<h1>Create list</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@list) do |f| %>
      <%= f.label :title %>
      <%= f.text_field :title, class: 'form-control'  %><br>

      <%= f.label :category %>
      <%= f.select :category, Taxi: 'Taxi', Traveling: 'Traveling', Clothing: 'Clothing', Cafes: 'Cafes', Shops: 'Shops', Other: 'Other'  %><br>
      <%= f.submit "Create list", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
