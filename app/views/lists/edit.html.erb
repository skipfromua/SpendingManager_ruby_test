<h1>Edit list</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@list) do |f| %>
      <%= f.label :title %>
      <%= f.text_field :title, class: 'form-control'  %><br>

      <%= f.label :category %>
      <%= f.text_field :category, class: 'form-control'  %><br>

      <%= f.submit "Update list", class: "btn btn-warning" %>
    <% end %>
  </div>
</div>
