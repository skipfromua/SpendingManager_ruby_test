<h1>Spendings new</h1>
<%= form_with(model: [@list, @spending], local: true) do |f| %>
  <%= f.label :description %>
  <%= f.text_field :description, class: 'form-control'  %><br>

  <%= f.label :money %>
  <%= f.number_field :money, class: 'form-control'  %><br>

  <%= f.submit "Create", class: "btn btn-primary" %>
<% end %>
<p>Find me in app/views/spendings/new.html.erb</p>
